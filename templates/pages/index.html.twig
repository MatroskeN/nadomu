                        <option value="default">Станция метро</option>
                        <option value="no-default">Станция метро</option>
                    </select>
                    <select name="specialty" id="specialty" class="custom_select">
                        <option value="default">Специализация</option>
                    </select>
                    <button class="choose">
                        Подобрать специалиста
                    </button>
                </div>

            </div>
            <img src="images/nurse.png" alt="nurse" class="nurse">
            <div class="ellipse"></div>
